#include<bits/stdc++.h>
#define ll long long int
using namespace std;
int main()
{
    int n,k,i,x,y;
    int t;
    cin >> n >> k;
    vector<int> a;
    for(i=0;i<n;i++)
    {
        cin >> t;
        a.push_back(t);
    }
    
    sort(a.begin(),a.end());
    if(k>n)
    {
        cout << -1 << endl;
    }
    else
    {
        x = a.at(a.size()-k);
        y = 0;
        cout << x << " " << y << endl;
    }

}
