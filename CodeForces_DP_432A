#include <iostream>
#include <vector>
#define ll long long int
using namespace std;
int main()
{
    int n,k,key;
    cin >> n >> k;
    key = 5 - k;
    int f[n];
    int c = 0;
    for(int i=0;i<n;i++)
    {
        cin >> f[i];
    }
    for(int i=0;i<n;i++)
    {
        if(f[i]<=key)
            c++;
    }
    cout << (c/3) << endl;
}
